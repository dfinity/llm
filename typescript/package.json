{
  "name": "@dfinity/llm",
  "version": "0.1.1",
  "description": "TypeScript library for interacting with DFINITY's LLM canister on the Internet Computer",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tsc --noEmit --strict",
    "type-check": "tsc --noEmit",
    "ci": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "dfinity",
    "internet computer",
    "ic",
    "llm",
    "ai"
  ],
  "author": "DFINITY Foundation",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfinity/llm.git"
  },
  "bugs": {
    "url": "https://github.com/dfinity/llm/issues"
  },
  "homepage": "https://github.com/dfinity/llm#readme",
  "dependencies": {
    "@dfinity/candid": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "azle": "^0.30.0-rc.0"
  },
  "peerDependencies": {
    "azle": ">=0.30.0-rc.0"
  }
}
